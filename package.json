{
  "name": "project2",
  "version": "1.0.0",
  "description": "HELLO WORLD",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Undeadmatrix/Project2.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "bugs": {
    "url": "https://github.com/Undeadmatrix/Project2/issues"
  },
  "homepage": "https://github.com/Undeadmatrix/Project2#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fomantic-ui": "^2.8.5",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7"
  }
}
